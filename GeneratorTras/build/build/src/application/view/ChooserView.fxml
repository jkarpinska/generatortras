<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<AnchorPane minHeight="200.0" minWidth="250.0" prefHeight="200.0" prefWidth="350.0" styleClass="background"
            stylesheets="@darktheme.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="application.view.ChooserViewController">
    <padding>
        <Insets bottom="20.0" left="25.0" right="25.0" top="20.0"/>
    </padding>
    <opaqueInsets>
        <Insets/>
    </opaqueInsets>
    <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="200.0" spacing="15.0" AnchorPane.bottomAnchor="0.0"
          AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <opaqueInsets>
            <Insets/>
        </opaqueInsets>
        <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="text" text="Wybierz zestaw tras:"/>
        <Button fx:id="btNewList" minWidth="200.0" mnemonicParsing="false" onAction="#createNewList"
                text="Stwórz nowy zestaw tras..."/>
        <Button fx:id="btListFromFile" minWidth="200.0" mnemonicParsing="false" onAction="#chooseFile"
                text="Wybierz zestaw z pliku..."/>
        <Button fx:id="btLastUsedList" minWidth="200.0" mnemonicParsing="false" onAction="#getLastUsedFile"
                text="Ostatnio używany"/>
    </VBox>
</AnchorPane>
